.cont {
  width: 100%;
  background: #f5f5f5;
  float: left;
}
.Header {
  width: 100%;
  display: flex;
  background: #fff;
}
.Header-deom {
  flex: 1;
  text-align: center;
  padding: 15rpx 0;
}
.Header-dlick {
  flex: 1;
  text-align: center;
  border-bottom: 9rpx solid #34aaff;
  padding: 15rpx 0;
}
.other-navigator-hover {
    color:red;
}
.order {
  width: 100%;
  height:100%;
  background: #f5f5f5;
  padding: 330rpx 0; 
  float: left;
  display: flex;
  justify-content: center;
}
.order text {
  width: 400rpx;
  float: left;
  text-align: center;
  margin-top: 20rpx;
}
.order image {
  width: 170rpx;
  height: 170rpx;
  margin-top: 20rpx;
  margin-left: 130rpx;
}
.order-background{
  width: 400rpx;
  height: 400rpx;
  border-radius: 50%;
  background: #e4e4e4;
}